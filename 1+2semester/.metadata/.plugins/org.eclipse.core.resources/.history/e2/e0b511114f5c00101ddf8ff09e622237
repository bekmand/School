package bil;
import java.util.ArrayList;

public class Udlejning {
	private int nr;
	private int antalDage;
	private String dato;
	private ArrayList<Udlejning> udlejning;
	
	public Udlejning(int nr, int antalDage, String dato){
		this.nr = nr;
		this.antalDage = antalDage;
		this.dato = dato;
	}
	/*
	public int getPris(){
		return 
	}
	*/
	public void setDage(int dage){
		this.antalDage = dage;
	}
	public int getDage(){
		return antalDage;h
	}
	public void addUdlejning(Udlejning udlejning){
		udlejning.add(udlejning);
	}
	public void removeUdlejning(Udlejning udlejning){
		udlejning.remove(udlejning);
	}
	

}
